{"components/search/show-more":"{{#if product_results}}\n  {{> components/faceted-search/show-more-facets product_results}}\n{{/if}}","components/faceted-search/show-more-facets":"<ul class=\"nav-list faceted-search-option-columns\" data-additional-facets-list>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li class=\"faceted-search-item\" data-facet-item>\n          <a href=\"{{url}}\"\n             class=\"nav-ist-action {{#if selected}} is-active {{/if}}\"\n             rel=\"nofollow\"\n             data-id=\"{{ id }}\"\n             data-faceted-search-facet>\n              <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n              <span class=\"faceted-search-item-label\">\n                {{title}}\n                {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n              </span>\n           </a></li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>\n"}
