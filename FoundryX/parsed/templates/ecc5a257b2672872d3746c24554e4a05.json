{"components/home/home-blog-section":"<section class=\"home-block home-blog-section blog-block\">\r\n\r\n  <div class=\"home-blog-section--container\">\r\n      <h2>{{blog.name}}</h2>\r\n      <div class=\"home-blog-list product-grid-container\">\r\n        {{#each blog.recent_posts}}\r\n            <div class=\"item\">\r\n              <div class=\"content\">\r\n                <h3>{{date_published}}</h3>\r\n                <h4>{{title}}</h4>\r\n                <p>{{summary}}...</p>\r\n                <a href=\"{{url}}\" class=\"btn arrow\">READ MORE<i class=\"crew-icon-thin-arrow-right\"></i></a>\r\n              </div>\r\n            </div>\r\n        {{/each}}\r\n      </div>\r\n  </div>\r\n\r\n</section>"}
