{"components/faceted-search/facets/range":"<form class=\"faceted-search-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}}\" method=\"get\" data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{ param_name }}\" value=\"{{ param_value }}\"/>\n  {{/each}}\n\n  <div class=\"facet-range-input-group\">\n    <div class=\"facet-range-inputs\">\n      <div class=\"form-field faceted-search-range-min\">\n        <input\n          class=\"form-input\"\n          name=\"min_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{ min_price }}\"\n          placeholder=\"{{ lang 'category.sidebar.min_price'}}\"\n          required\n        />\n      </div>\n\n      <div class=\"form-field faceted-search-range-max\">\n        <input\n          class=\"form-input\"\n          name=\"max_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{ max_price }}\"\n          placeholder=\"{{ lang 'category.sidebar.max_price'}}\"\n          required\n        />\n      </div>\n    </div>\n\n    <input class=\"button button-primary button-small facet-submit\" type=\"submit\" value=\"{{lang 'category.sidebar.update'}}\">\n\n  </div>\n</form>\n"}
