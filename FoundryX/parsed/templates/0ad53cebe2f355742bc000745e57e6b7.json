{"components/common/pagination":"<div class=\"pagination-container\">\n  <div class=\"pagination\">\n    {{#if previous}}\n      <a class=\"pagination-item previous\" href=\"{{previous}}\" rel=\"nofollow\" data-faceted-search-facet>\n        <i class=\"icon-arrow-left\" aria-hidden=\"true\"></i>\n        {{lang 'common.pagination.previous'}}\n      </a>\n    {{/if}}\n\n    <output class=\"pagination-count\">\n      {{#if links.length '>' 1}}\n        {{#each links}}\n          {{#if number '===' ../current}}\n            <span class=\"pagination-number pagination-current\"><a href=\"{{url}}\">{{number}}</a></span>\n          {{else}}\n            <span class=\"pagination-number\"><a href=\"{{url}}\">{{number}}</a></span>\n          {{/if}}\n        {{/each}}\n      {{else}}\n        <span class=\"pagination-number pagination-current\">1</span>\n      {{/if}}\n    </output>\n\n    {{#if next}}\n      <a class=\"pagination-item next\" href=\"{{next}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{lang 'common.pagination.next'}}\n        <i class=\"icon-arrow-right\" aria-hidden=\"true\"></i>\n      </a>\n    {{/if}}\n  </div>\n</div>\n"}
